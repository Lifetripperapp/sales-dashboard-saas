{
  "name": "uytech-saas",
  "version": "1.0.0",
  "description": "UYTECH Sales Dashboard - SaaS Multi-Tenant Version",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "nodemon server.js",
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css}'",
    "db:setup": "node scripts/setup-db.js",
    "db:setup:supabase": "node scripts/supabase-setup.js setup",
    "db:migrate:supabase": "node scripts/supabase-setup.js migrate",
    "db:extract": "node scripts/extract-data.js",
    "backup:create": "node scripts/backup/create-backup.js",
    "backup:restore": "node scripts/backup/restore-backup.js",
    "heroku:deploy": "git push heroku main",
    "vercel:deploy": "vercel --prod",
    "stripe:test": "node scripts/test-stripe.js",
    "auth0:test": "node scripts/test-auth0.js",
    "webhook:test-registration": "curl -X POST http://localhost:3000/api/auth/webhook/user-registered -H 'Content-Type: application/json' -d '{\"event\":\"user_registered\",\"user\":{\"email\":\"test@uytech.com.uy\",\"name\":\"Test User\"},\"timestamp\":\"2025-07-12T16:38:00.000Z\"}'",
    "docs:review": "node scripts/documentation-agent.js",
    "docs:generate": "node scripts/documentation-agent.js"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "sequelize": "^6.35.0",
    "stripe": "^14.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uytech/uytech-saas"
  },
  "keywords": [
    "saas",
    "multi-tenant",
    "sales-dashboard",
    "react",
    "supabase",
    "stripe",
    "auth0"
  ],
  "author": "UYTECH Team",
  "license": "MIT"
}
